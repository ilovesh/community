<% provide(:title, 'Edit list') %>
<div class="row center">
  <div class="span6 offset4">
    <%= form_for(@list) do |f| %>
      <%= render 'fields', f: f %>
      <%= render 'shared/error_messages', object: f.object %>
      <%= f.submit "update the list", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>