<% provide(:title, 'All lists') %>
<% content_for :left do %>
  <h1>All lists</h1>
  <%= link_to "create a list",  new_list_path, class: "btn btn-large btn-primary" %>
  <%= will_paginate @lists %>
  <ul class="all_lists">
    <%= render partial: 'shared/list', collection: @lists %>
  </ul>
  <%= will_paginate @lists %>
<% end %>

<% content_for :right do %>
  place_holder
<% end %>