    <div class="accordion-group">
      <div class="accordion-heading">
        <a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion2" href="#discussion-likes">
          <h3>Voted for <%= pluralize(@liked_discussions.count, 'discussion') %></h3>
        </a>
      </div>
      <div id="discussion-likes" class="accordion-body collapse">
        <div class="accordion-inner">         
          <ul class="unstyled seperate">
            <%= render partial: 'discussions/discussion', collection: @liked_discussions, as: :discussion %>
          </ul>
        </div>
      </div>
    </div>